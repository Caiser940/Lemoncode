: red;\r\n$bgcolorgreen: green;\r\n$bgcolorblue: blue;\r\n$bgcolororange: orange;\r\n\r\ndiv {\r\n  .box {\r\n    width: auto;\r\n    height: auto;\r\n    border-radius: 10%;\r\n    padding: 2rem;\r\n    margin: 0.2rem 0.5rem;\r\n    text-align: center;\r\n  }\r\n}\r\n\r\n@for $x from 1 through 4 {\r\n  .container-#{$x} {\r\n    display: flex;\r\n    align-items: center;\r\n    @if $x == 1 {\r\n      @for $i from 1 through 4 {\r\n        .darken-#{$i} {\r\n          $a: $i * 10%;\r\n          background-color: darken($bgcolorred, $a);\r\n        }\r\n        .lighten-#{$i} {\r\n          $a: $i * 10%;\r\n          background-color: lighten($bgcolorred, $a);\r\n        }\r\n      }\r\n      .base-color {\r\n        background-color: $bgcolorred;\r\n      }\r\n    } @else if $x == 2 {\r\n      @for $i from 1 through 4 {\r\n        .darken-#{$i} {\r\n          $a: $i * 10%;\r\n          background-color: darken($bgcolorblue, $a);\r\n        }\r\n        .lighten-#{$i} {\r\n          $a: $i * 10%;\r\n          background-color: lighten($bgcolorblue, $a);\r\n        }\r\n      }\r\n      .base-color {\r\n        background-color: $bgcolorblue;\r\n      }\r\n    } @else if $x == 3 {\r\n      @for $i from 1 through 4 {\r\n        .darken-#{$i} {\r\n          $a: $i * 10%;\r\n          background-color: darken($bgcolorgreen, $a);\r\n        }\r\n        .lighten-#{$i} {\r\n          $a: $i * 10%;\r\n          background-color: lighten($bgcolorgreen, $a);\r\n        }\r\n      }\r\n      .base-color {\r\n        background-color: $bgcolorgreen;\r\n      }\r\n    } @else if $x == 4 {\r\n      @for $i from 1 through 4 {\r\n        .darken-#{$i} {\r\n          $a: $i * 10%;\r\n          background-color: darken($bgcolororange, $a);\r\n        }\r\n        .lighten-#{$i} {\r\n          $a: $i * 10%;\r\n          background-color: lighten($bgcolororange, $a);\r\n        }\r\n      }\r\n      .base-color {\r\n        background-color: $bgcolororange;\r\n      }\r\n    } @else {\r\n      @for $i from 1 through 4 {\r\n        .darken-#{$i} {\r\n          $a: $i * 10%;\r\n          background-color: darken($bgcolor, $a);\r\n        }\r\n        .lighten-#{$i} {\r\n          $a: $i * 10%;\r\n          background-color: lighten($bgcolor, $a);\r\n        }\r\n      }\r\n      .base-color {\r\n        background-color: $bgcolor;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n"},"lineCount":null}},"error":null,"hash":"be682606465c1d55bb7345e7d714fb6d","cacheData":{"env":{}}}